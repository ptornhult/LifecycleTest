        </activity>

        <activity android:name=".test1.Test1Activity" android:label="Activity + ViewModel" />
        <activity android:name=".test2.Test2Activity" android:label="Fragment + ViewModel" />
        <activity android:name=".test3.Test3Activity" android:label="Fragment + BackStack + ViewModel" />
    </application>

</manifest>